        "autoprefixer": "^10.4.19",
        "node-fetch": "^3.3.2",
        "postcss": "^8.4.38",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "vite-tsconfig-paths": "^4.2.1",
        "wrangler": "^3.48.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
